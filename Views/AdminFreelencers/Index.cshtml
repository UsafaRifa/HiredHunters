@model IEnumerable<HiredHunters.Models.Freelencer>

@{
    ViewBag.Title = "Admin Dashboard";
    Layout = "~/Views/Shared/Admin_layout.cshtml";
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>
 
    <table class="table">
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Freelencer_ID)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.FirstName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.LastName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.PhoneNumber)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Email)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.DateofBirth)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.F_Address)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.DateofJoining)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.JobCompleted)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.NetIncome)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.rating)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.username)
            </th>
    
            <th></th>
        </tr>
    
    @foreach (var item in Model) {
        <tr class="table-primary">
            <td>
                @Html.DisplayFor(modelItem => item.Freelencer_ID)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FirstName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.LastName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.PhoneNumber)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Email)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DateofBirth)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.F_Address)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DateofJoining)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.JobCompleted)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.NetIncome)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.rating)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.username)
            </td>
        
          
            <td>
               
                @Html.ActionLink("Delete", "Delete", new { id=item.f_no }, new { @class = "btn btn - primary" })
            </td>
        </tr>
    }
    
    </table>
</body>
</html>
